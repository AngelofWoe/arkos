#!/bin/sh

set -e

# Pass maintainer script parameters to hook scripts
export DEB_MAINT_PARAMS="$*"

# Tell initramfs builder whether it's wanted
export INITRD=Yes

test -d /etc/kernel/postrm.d && run-parts --arg="4.4.189-139502-g380eeff98d35" --arg="/boot/vmlinuz-4.4.189-139502-g380eeff98d35" /etc/kernel/postrm.d
exit 0
